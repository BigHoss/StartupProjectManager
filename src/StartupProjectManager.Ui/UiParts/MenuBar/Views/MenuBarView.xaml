<UserControl xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
             x:Class="StartupProjectManager.Ui.UiParts.MenuBar.Views.MenuBarView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:cal="http://www.caliburnproject.org"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <syncfusion:MenuAdv >
            <syncfusion:MenuItemAdv Header="File">
                <syncfusion:MenuItemAdv Header="New"
                                        cal:Message.Attach="[Event Click] = [Action FileMenuNew()]">
                    <syncfusion:MenuItemAdv.Icon>
                        <Image Source="../../../Images/Add.png"/>
                    </syncfusion:MenuItemAdv.Icon>
                </syncfusion:MenuItemAdv>
            </syncfusion:MenuItemAdv>
        </syncfusion:MenuAdv>
    </Grid>
</UserControl>
